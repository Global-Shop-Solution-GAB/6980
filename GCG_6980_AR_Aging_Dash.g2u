Program.Sub.GSGCAging_RowCellClick.Start
	V.Local.iRet.Declare()
		
	V.Local.sInvoice.Declare()
	V.Local.sReference.Declare()
	V.Local.sReferencePrefix.Declare()
	V.Local.sQuery.Declare()
	V.Local.sType.Declare()
	
	F.Intrinsic.Control.Try
		
		F.Intrinsic.Control.BlockEvents()
		
			Gui.FrmAging.GSGCAging.GetCellValueByColumnName("gvAging", "INVOICE", V.Args.RowIndex, V.Local.sInvoice)
			Gui.FrmAging.GSGCAging.GetCellValueByColumnName("gvAging", "REFERENCE", V.Args.RowIndex, V.Local.sReference)
			Gui.FrmAging.GSGCAging.GetCellValueByColumnName("gvAging", "TYPE", V.Args.RowIndex, V.Local.sType)
			

			' If the user clicked on the Adjust column
			F.Intrinsic.Control.If(V.Args.Column, =, "ADJUST", AND, V.Screen.FrmAging!optEdit.Value, =, True)
				
				F.Global.General.LaunchMenuTask(93, 2, V.Local.iRet)	
				
			F.Intrinsic.Control.EndIf
			
			' ElseIf the user clicked on the Pay column
			F.Intrinsic.Control.If(V.Args.Column, =, "PAY", AND, V.Screen.FrmAging!optEdit.Value, =, True)
				
				F.Intrinsic.Control.If(V.Local.sType, =, "Invoice")
					F.Global.General.LaunchMenuTask(93, 2, V.Local.iRet)
				F.Intrinsic.Control.EndIf
						
			F.Intrinsic.Control.EndIf
			
			' ElseIf the user clicked on the Invoice column
			F.Intrinsic.Control.If(V.Args.Column, =, "INVOICE", AND, V.Global.bPrintInvoice, =, True)
				
				F.Intrinsic.Control.If(V.Local.sType, =, "Invoice", OR, V.Local.sType, =, "Cash")
				
					F.Intrinsic.String.Build("SELECT COUNT(1) FROM v_order_hist_line WHERE invoice = '{0}'", V.Local.sInvoice, V.Local.sQuery)
					F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sQuery, V.Local.iRet)
					
					F.Intrinsic.Control.If(V.Local.iRet, >, 0)
						F.Global.General.CallWrapperASync(200700, V.Local.sInvoice)
					F.Intrinsic.Control.Else
						F.Intrinsic.UI.Msgbox("The invoice could not be printed.")
					F.Intrinsic.Control.EndIf
				
				F.Intrinsic.Control.ElseIf(V.Local.sType, =, "Credit Memo")
				
					F.Intrinsic.String.Left(V.Local.sReference, 5, V.Local.sReferencePrefix)
					
					F.Intrinsic.Control.If(V.Local.sReferencePrefix, =, "CM # ")
						F.Intrinsic.String.Split(V.Local.sReference, " ", V.Local.sReference)
						F.Global.General.CallWrapperASync(200905, V.Local.sReference(2))
					F.Intrinsic.Control.Else
						Function.Intrinsic.UI.Msgbox("The credit memo could not be printed.")
					F.Intrinsic.Control.EndIf
				
				F.Intrinsic.Control.EndIf
				
			F.Intrinsic.Control.EndIf
			
			' Else, for all other columns, do nothing.
		
		F.Intrinsic.Control.UnBlockEvents()
	
	F.Intrinsic.Control.Catch
		
		V.Local.sError.Declare()
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}", V.Ambient.ScriptPath, V.Ambient.Newline, V.Ambient.CurrentSubroutine, V.Ambient.ErrorNumber, V.Ambient.ErrorDescription, V.Ambient.GABVersion, V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End()
		
	F.Intrinsic.Control.Finally
	
		F.Intrinsic.Control.UnblockEvents()
		
	F.Intrinsic.Control.EndTry
	
Program.Sub.GSGCAging_RowCellClick.End

