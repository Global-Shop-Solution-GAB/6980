	F.Intrinsic.String.Format(V.Local.dStatementDate,"MMDDYY",v.Local.sStatementDate)
	V.Local.sCustomerNumberParam.Set(V.Args.CustomerNumber)
	V.Local.sFilePath.Set(V.Args.FilePath)
	V.Local.sCustomerName.Set(V.Args.CustomerName)
	
	F.Intrinsic.Control.If(V.DataTable.dtPDFLog.Exists, =, False)
		F.Data.DataTable.Create("dtPDFLog", True)
		F.Data.DataTable.AddColumn("dtPDFLog", "FilePath", "String")
		F.Data.DataTable.AddColumn("dtPDFLog", "CustomerNumber", "String")
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Build("CustomerNumber = '{0}'", V.Local.sCustomerNumberParam, V.Local.sVal)
		F.Data.DataTable.Select("dtPDFLog", V.Local.sVal, V.Local.sRet)
		F.Intrinsic.Control.If(V.Local.sRet, <>, "***NORETURN***")
			f.Intrinsic.Variable.AddRV("PDFExists",True)
			
			F.Intrinsic.Control.ExitSub
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
	
	'Get Titles
	F.ODBC.Connection!con.ExecuteAndReturn("Select TEXT1 From V_OP_HEADER WHERE ID = '401069' OR ID = '401070' OR ID = '401071'",V.Local.sTitles)
	F.Intrinsic.String.Split(V.Local.sTitles,"#$#",V.Local.sTitles)
	
	'Set Variables
	F.Global.BI.GetRunID(V.Local.iBIRunID)
	F.Global.BI.StartLogging(V.Local.iBIRunID,562,0,"",V.Local.iLogID)
	'Create statement with CallWrapper
			'Uses customer number without double appostrophie
			F.Intrinsic.String.Build("{0}!*!{0}!*!{1}!*!Y!*!N!*!N!*!{3}!*!{2}!*!{2}!*!N!*!N",V.Local.sCustomerNumberParam,V.local.sStatementDate," ",V.Local.sStatementDate,V.Local.sParam)
			F.Global.General.CallWrapperSyncBIO(3050,V.Local.sParam)
			'Export and save PDF
			V.Local.sParam.Set("Terminal*!*ReportID*!*ATTN*!*Title1*!*Title2*!*Title3*!*Title4*!*Logo")
			V.Local.sParam.Set("Terminal*!*ReportID*!*ATTN*!*Title1*!*Title2*!*Title3*!*Title4")
			F.Intrinsic.String.Build("{0}*!*000562*!*Y*!*{1}*!*{2}*!*{3}*!*{4}*!*{5}",V.Caller.Terminal,"Current",V.Local.sTitles(0),V.Local.sTitles(1),V.Local.sTitles(2),V.Local.sVal)
			F.Global.BI.RunReportPreProcessor(V.Local.iBIRunID,V.Local.iLogID,V.Local.sParam,V.Local.sVal,"",4,True,"",-1,"",0,V.Local.sFilePath,"",V.Local.iRet)
			
			'check to make sure file exists.
			F.Intrinsic.File.Exists(V.Local.sFilePath, V.Local.bExists)			
			F.Intrinsic.Control.If(V.Local.bExists.Not)
				F.Intrinsic.String.build("Statement not created at {0}. Please try again. If issue persists, please contact Global Shop Solutions.",v.Local.sFilePath,v.Local.sVal)
				f.Intrinsic.UI.Msgbox(v.Local.sVal,"Statement Generation Error...")				
			Function.Intrinsic.Control.Else		
	
				'Check PDF Text and make sure the page is not blank
				F.Automation.PDF.Open("STATEMENT",V.Local.sFilePath)
				F.Automation.PDF.TextSearch("STATEMENT",V.Local.sCustomerName,V.Local.sPDFText)
				F.Automation.PDF.Close("STATEMENT")
				F.Intrinsic.Control.If(V.Local.sPDFText, =, "***NO RETURN***")
					v.Local.bExists.Set(False)
					F.Intrinsic.String.build("Statement generated at {0}, but was blank. Please try again. If issue persists, please contact Global Shop Solutions.",v.Local.sFilePath,v.Local.sVal)
				f.Intrinsic.UI.Msgbox(v.Local.sVal,"Statement Generation Error...")	
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf					
			
			F.Intrinsic.Control.If(V.Local.bExists)
				F.Data.DataTable.AddRow("dtPDFLog", "CustomerNumber", V.Local.sCustomerNumberParam, "FilePath", V.Local.sFilePath)
			F.Intrinsic.Control.EndIf
			f.Intrinsic.Variable.AddRV("PDFExists",v.Local.bExists)			

'Stop BI Logging
	F.Global.BI.StopLogging(V.Local.iLogID)
	

F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.GenerateStatementPDF.End

