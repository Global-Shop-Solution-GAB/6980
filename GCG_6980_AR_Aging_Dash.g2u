	Gui.FrmAging.GSGCStatements.SetColumnProperty("gvStatements","SEL","HeaderHAlignment","Center")
	Gui.FrmAging.GSGCStatements.SetColumnProperty("gvStatements","SEL","CellHAlignment","Center")
	
	'CUSTOMER
